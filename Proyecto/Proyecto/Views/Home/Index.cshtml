<body id="top" data-spy="scroll" data-target=".navbar-collapse" data-offset="50">

    <div class="container">

        <!-- PRE LOADER -->
        <section class="preloader">
            <div class="spinner">

                <span class="spinner-rotate"></span>

            </div>
        </section>


        <!-- MENU -->
        <section class="navbar custom-navbar navbar-fixed-top" role="navigation">
            <div class="container">

                <div class="navbar-header">
                    <button class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon icon-bar"></span>
                        <span class="icon icon-bar"></span>
                        <span class="icon icon-bar"></span>
                    </button>

                    <!-- lOGO TEXT HERE -->
                    <a href="#" class="navbar-brand">Informática Empresarial</a>

                </div>

                <!-- MENU LINKS -->
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-nav-first">
                        <li><a href="#top" class="smoothScroll">Inicio</a></li>
                        <li><a href="#about" class="smoothScroll">Acerca de</a></li>
                        <li><a href="#contact" class="smoothScroll">Contáctanos</a></li>
                        <button type="button" id="btnLog" class="btn" onclick="clearTextBoxStudent();">Registrarse</button>
                        <button type="button" id="btnSignLog" data-toggle="modal" data-target="#myModalSignLog" class="btn">Iniciar Sesión</button>
                        <button type="button" id="btnProfessor" onclick="clearTextBoxProfessor();" class="btn">Registrar profesor nuevo</button>
                        <button type="button" id="btnCourse" data-toggle="modal" onclick="clearTextBoxCourse();" data-target="#myModalCourse" class="btn">Registrar curso nuevo</button>
                    </ul>
                </div>

            </div>
        </section>


        <!---Sign up log in-->

        <div class="modal fade" id="myModalSignLog" tabindex="-1" role="dialog" hidden>

            <div class="modal-dialog">

                <div class="modal-content">

                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal">X</button>
                        <h4 id="HeaderTitleAdd" class="modal-title"> Inicio de sesión </h4>

                    </div>
                    <div class="modal-body">

                        <form>
                            <div class="form-group">
                                <label for="UsernameSignLog">Nombre de usuario</label>
                                <input type="text" class="form-control" id="UsernameSignLog" placeholder="Ingrese su usuario" autocomplete="off">

                            </div>

                            <div class="form-group">
                                <label for="PasswordSignLog">Contraseña</label>
                                <input type="password" class="form-control" id="PasswordSignLog" placeholder="Ingrese su contraseña" autocomplete="off">
                            </div>

                        </form>
                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" onclick="login();" id="btnAdd">Iniciar sesión</button>
                        <button type="button" class="btn btn-default" id="btnClose" data-dismiss="modal">Cerrar</button>
                    </div>
                </div>
            </div>
        </div>




        <!---Student Register-->

        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" hidden>

            <div class="modal-dialog modal-lg">

                <div class="modal-content">

                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal">X</button>
                        <h4 id="HeaderTitleAdd" class="modal-title"> Registro de estudiante </h4>

                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-6">
                                <form>
                                    <div class="form-group">
                                        <label for="StudentCard">Carné</label>
                                        <input type="text" class="form-control" id="StudentCard" placeholder="Carné">

                                    </div>

                                    <div class="form-group">
                                        <label for="Name">Nombre</label>
                                        <input type="text" class="form-control" id="Name" placeholder="Nombre" autocomplete="off">
                                    </div>


                                    <div class="form-group">
                                        <label for="Last Name">Apellido</label>
                                        <input type="text" class="form-control" id="LastName" placeholder="Apellido" value"reset">
                                    </div>

                                    <div class="form-group">
                                        <label for="Birthday">Fecha de Nacimiento</label>
                                        <input type="date" class="form-control" id="Birthday" placeholder="Fecha de Nacimiento">
                                    </div>

                                    <div class="form-group">
                                        <label for="Mail">Correo eléctronico</label>
                                        <input type="text" class="form-control" id="Mail" placeholder="Correo eléctronico">
                                    </div>

                                    <div class="form-group">
                                        <label for="Image">Fotografía</label>
                                        <input type="file" class="form-control" id="Image" placeholder="Fotografía">
                                    </div>
                                </form>
                            </div>
                            <div class="col-md-6">
                                <form>
                                    <div class="form-group">
                                        <label for="Username">Nombre de usuario</label>
                                        <input type="text" class="form-control" id="Username" placeholder="Nombre de usuario">
                                    </div>

                                    <div class="form-group">
                                        <label for="Password">Contraseña</label>
                                        <input type="password" class="form-control" id="Password" placeholder="Contraseña">
                                    </div>

                                    <div class="form-group">
                                        <label for="Province">Provincia</label>
                                        <select class="form-control" id="ProvinceDropdown" name="ProvinceDropdown"></select>
                                    </div>

                                    <div class="form-group">
                                        <label for="Canton">Cantón</label>
                                        <select class="form-control" id="CantonDropdown" name="CantonDropdown"></select>
                                    </div>

                                    <div class="form-group">
                                        <label for="District">Distrito</label>
                                        <select class="form-control" id="DistrictDropdown" name="DistrictDropdown"></select>
                                    </div>
                                </form>

                            </div>
                        </div>

                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" id="btnAdd" onclick="return Add();">Registrarse</button>
                        <button type="button" class="btn btn-primary" id="btnUpdate" style="display:none" onclick="Update();">Actualizar</button>
                        <button type="button" class="btn btn-default" id="btnClose" data-dismiss="modal">Cerrar</button>

                    </div>
                </div>
            </div>
        </div>

        <!---Professor Register-->

        <div class="modal fade" id="myModalProfessor" tabindex="-1" role="dialog" hidden>

            <div class="modal-dialog modal-lg">

                <div class="modal-content">

                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal">X</button>
                        <h4 id="HeaderTitleAdd" class="modal-title"> Registro de profesor </h4>

                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-6">
                                <form>
                                    <div class="form-group">
                                        <label for="ProfessorUsername">Nombre de usuario</label>
                                        <input type="text" class="form-control" id="ProfessorUsername" placeholder="Nombre de usuario" autocomplete="off">

                                    </div>

                                    <div class="form-group">
                                        <label for="ProfessorPassword">Contraseña</label>
                                        <input type="password" class="form-control" id="ProfessorPassword" placeholder="Contraseña" autocomplete="off">
                                    </div>

                                    <div class="form-group">
                                        <label for="ProfessorName">Nombre</label>
                                        <input type="text" class="form-control" id="ProfessorName" placeholder="Nombre" autocomplete="off">
                                    </div>

                                    <div class="form-group">
                                        <label for="ProfessorLastName">Apellido</label>
                                        <input type="text" class="form-control" id="ProfessorLastName" placeholder="Apellido" autocomplete="off">
                                    </div>

                                    <div class="form-group">
                                        <label for="ProfessorMail">Correo eléctronico</label>
                                        <input type="text" class="form-control" id="ProfessorMail" placeholder="Correo eléctronico" autocomplete="off">
                                    </div>

                                </form>
                            </div>
                            <div class="col-md-6">
                                <form>
                                    <div class="form-group">
                                        <label for="ProfessorImage">Fotografía</label>
                                        <input type="file" class="form-control" id="ProfessorImage">
                                    </div>

                                    <div class="form-group">
                                        <label for="AcademicDegree">Grado Académico</label>
                                        <select class="form-control" id="AcademicDegreeDropdown" name="AcademicDegreeDropdown"></select>
                                    </div>


                                    <div class="form-group">
                                        <label for="Province">Provincia</label>
                                        <select class="form-control" id="ProfessorProvinceDropdown" name="ProvinceDropdown"></select>
                                    </div>

                                    <div class="form-group">
                                        <label for="Canton">Cantón</label>
                                        <select class="form-control" id="ProfessorCantonDropdown" name="CantonDropdown"></select>
                                    </div>

                                    <div class="form-group">
                                        <label for="District">Distrito</label>
                                        <select class="form-control" id="ProfessorDistrictDropdown" name="DistrictDropdown"></select>
                                    </div>
                                </form>

                            </div>
                        </div>

                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" id="btnAdd" onclick="return AddProfessor();">Registrar</button>
                        <button type="button" class="btn btn-default" id="btnClose" data-dismiss="modal">Cerrar</button>

                    </div>
                </div>
            </div>
        </div>


        <!--Course Register---->
        <div class="modal fade" id="myModalCourse" tabindex="-1" role="dialog" hidden>

            <div class="modal-dialog">

                <div class="modal-content">

                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal">X</button>
                        <h4 id="HeaderTitleAdd" class="modal-title"> Registro de curso nuevo </h4>

                    </div>
                    <div class="modal-body">
                        <form>

                            <div class="form-group">
                                <label for="CourseInitials">Siglas</label>
                                <input type="text" class="form-control" id="CourseInitials" placeholder="Ingrese las siglas del curso" autocomplete="off">
                            </div>

                            <div class="form-group">
                                <label for="CourseName">Nombre</label>
                                <input type="text" class="form-control" id="CourseName" placeholder="Ingrese el nombre del curso" autocomplete="off">
                            </div>

                            <div class="form-group">
                                <label for="IsCourseActive">Estado de curso</label>
                                <select class="form-control" id="IsActiveDropdown" name="IsActiveDropdown">
                                    <option value="0">Inactivo</option>
                                    <option value="1">Activo</option>
                                </select>
                            </div>

                            <div class="form-group">
                                <label for="CourseCredits">Créditos</label>
                                <input type="text" class="form-control" id="CourseCredits" placeholder="Ingrese los créditos del curso" autocomplete="off">
                            </div>

                            <div class="form-group">
                                <label for="CourseCycle">Ciclo</label>
                                <select class="form-control" id="CycleDropdown" name="CycleDropdown">
                                    <option value="1">I Semestre</option>
                                    <option value="2">II Semestre</option>
                                    <option value="3">Verano</option>
                                </select>
                            </div>

                        </form>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-primary" onclick="addCourse();" id="btnAddCourse">Registrar curso</button>
                            <button type="button" class="btn btn-default" onclick="editCourse();" id="btnEditCourse" data-dismiss="modal">Actualizar</button>
                            <button type="button" class="btn btn-default" id="btnClose" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- HOME -->
        <section id="home">
            <div class="row">

                <div class="owl-carousel owl-theme home-slider">
                    <div class="item item-first">
                        <div class="caption">
                            <div class="container">
                                <div class="col-md-6 col-sm-12">
                                    <h1>Asociación de Estudiantes</h1>
                                    <h3>We have the student association which helps other students in academic and recreational activities.</h3>
                                    <a href="#feature" class="section-btn btn btn-default smoothScroll">Discover more</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="item item-second">
                        <div class="caption">
                            <div class="container">
                                <div class="col-md-6 col-sm-12">
                                    <h1>Talks and workshops</h1>
                                    <h3>We provide hands-on workshops and talks for students to learn about more topics outside of the career curriculum.</h3>
                                    <a href="#courses" class="section-btn btn btn-default smoothScroll">Take a course</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="item item-third">
                        <div class="caption">
                            <div class="container">
                                <div class="col-md-6 col-sm-12">
                                    <h1>Adequate infrastructure</h1>
                                    <h3>We offer a good infrastructure for the development of each lesson.</h3>
                                    <a href="#contact" class="section-btn btn btn-default smoothScroll">See gallery</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>


        <!------Student Section-->

        <section id="studentSection" hidden>
            <div class="row">
                <h1>Estudiante</h1>
                <h2>Este será el acceso a los estudiantes.</h2>
