                    </table>
                </div>

                <!---ADMINISTRATOR/LIST PROFESSOR-->
                <div class="table-responsive hidden">
                    <h3>Profesores Registrados</h3>
                    <table class="table table-bordered table-hover" id="tableProfesor">

                        <thead>
                            <tr>
                                <th>
                                    ID
                                </th>
                                <th>
                                    Name
                                </th>
